{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:3000",
    "distDir": "../.output/public"
  },
  "package": {
    "productName": "Catenam",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": true,
        "execute": true,
        "open": true,
        "scope": [
          {
            "name": "run-git",
            "cmd": "git",
            "args": true
          },
          {
            "name": "run-adb",
            "cmd": "adb",
            "args": true
          },
          {
            "name": "run-ping",
            "cmd": "ping",
            "args": true
          },
          {
            "name": "run-winget",
            "cmd": "winget",
            "args": true
          },
          {
            "name": "run-java",
            "cmd": "java",
            "args": true
          },
          {
            "name": "run-powershell",
            "cmd": "powershell",
            "args": true
          }
        ],
        "sidecar": true
      },
      "fs": {
        "all": true,
        "copyFile": true,
        "createDir": true,
        "exists": true,
        "readDir": true,
        "readFile": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "scope": ["$HOME/Desktop/test/*"],
        "writeFile": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "Junhsiun",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 400,
        "resizable": true,
        "title": "Catenam",
        "width": 500,
        "transparent": true,
        "titleBarStyle": "Transparent",
        "decorations": false
      },
      {
        "label": "bar",
        "height": 100,
        "width": 350,
        "transparent": true,
        "titleBarStyle": "Transparent",
        "decorations": false,
        "title": "Catenam",
        "resizable": true,
        "closable": false,
        "fullscreen": false,
        "minimizable": false,
        "maximizable": false,
        "alwaysOnTop": true,
        "focus": true,
        "hiddenTitle": true,
        "skipTaskbar": true,
        "visible": false,
        "url": "/barPage"
      }
    ]
  }
}
